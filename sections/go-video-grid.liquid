{% schema %}
{
  "name": "go video grid",
  "tag": "section",
  "class": "section",
  "settings": [
    {
        "type": "text",
        "id": "title",
        "label": "Header",
        "default": "GOSOAKY TV"
      }
  
  ],

  "blocks": [
    {
      "name": "Video",
      "type": "video",
      "settings": [
        {
          "type": "image_picker",
          "id": "thumbnail",
          "label": "thumbnail"
        },
        {
            "type": "url",
            "id": "video_url",
            "label": "url"
        }
      ]
    }
 ],

  "presets": [
    {
      "name": "go video grid"
    }
  ]
}
{% endschema %}