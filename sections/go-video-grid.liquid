<div class="go-section-gosoakytv">
  <div class="go-gosoakytv-container"><h2 class="go-h2-title">This is GOSOAKY tv</h2></div>
  <div class="go-gosoakytv-grid">
    
    {% for block in section.blocks %}
        {% case block.type %}
          {% when 'video' %}
            <a
                href="#"
                class="go-gosoakytv-lightbox-link w-inline-block w-lightbox"
                aria-label="open lightbox"
                aria-haspopup="dialog"
              >
                <div class="go-play-button-block">
                  <img
                    src="https://cdn.shopify.com/s/files/1/1012/6158/files/Triangle-GOSOAKY-black.png?v=1660919262"
                    loading="lazy"
                    alt=""
                    class="go-play-button-png"
                  >
                </div>
                <img
                  src="{{ block.settings.thumbnail | image_url }}"
                  loading="lazy"
                  alt="video thumbnail"
                  class="go-lightbox-thumbnail"
                >
                <script type="application/json" class="w-json">
                  {
                    "items": [
                      {
                        "url": "{{ block.settings.video_url }}",
                        "originalUrl": "{{ block.settings.video_url }}",
                        "width": 940,
                        "height": 496,
                        "thumbnailUrl": "{{ block.settings.thumbnail | image_url }}",
                        "html": "<iframe class=\"embedly-embed\" src=\"{{ block.settings.video_url | url_escape }}\" width=\"940\" height=\"496\" scrolling=\"no\" title=\"Vimeo embed\" frameborder=\"0\" allow=\"autoplay; fullscreen\" allowfullscreen=\"true\"></iframe>",
                        "type": "video"
                      }
                    ],
                    "group": "gosoaky-tv"
                  }
                </script></a
              >
         
        {% endcase %}
      {% endfor %}
    
    
        
